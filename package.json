{
  "name": "spb_server",
  "version": "0.0.1",
  "description": "projeto Servidor de mensageria SPB ",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --exit-child --inspect --transpile-only --ignore-watch node_modules --respawn src/server.ts",
    "migrate": "prisma migrate dev",
    "start:prod": "node dist/server.js",
    "start:migrate:prod": "prisma migrate deploy && npm run start:prod",
    "reset": "prisma migrate reset",
    "lint": "eslint src --ext .ts ",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "dependencies": {
    "@prisma/client": "^4.4.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-fileupload": "^1.4.0",
    "express-xml-bodyparser": "^0.3.0",
    "jsonwebtoken": "^8.5.1",
    "libxmljs2": "^0.30.1",
    "morgan": "^1.10.0",
    "path": "^0.12.7",
    "pdf-parse": "^1.1.1",
    "pdf.js-extract": "^0.2.0",
    "uuid": "^9.0.0",
    "winston": "^3.8.1",
    "winston-daily-rotate-file": "^4.7.1",
    "winston-prisma-transporter": "^0.3.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.2.3",
    "@types/express-xml-bodyparser": "^0.3.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.0.0",
    "@types/pdf-parse": "^1.1.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "prisma": "^4.4.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4",
    "xslt3": "^2.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MiguelDuqueFilho/sbp_server.git"
  },
  "keywords": [
    "nodejs",
    "express",
    "typescript"
  ],
  "author": "Miguel Duque Filho <miguel@duquebr.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MiguelDuqueFilho/sbp_server/issues"
  },
  "homepage": "https://github.com/MiguelDuqueFilho/sbp_server#readme"
}
